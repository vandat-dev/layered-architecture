        "/delivery-frame/upload": {
            "post": {
                "description": "Upload an image to MinIO and update the scan record",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "delivery-frame"
                ],
                "summary": "Upload an image for a scan",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Device ID",
                        "name": "device_id",
