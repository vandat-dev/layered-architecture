                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Scan ID",
                        "name": "scan_id",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "Image File",
                        "name": "image",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
